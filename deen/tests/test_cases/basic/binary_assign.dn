fn main() i32 {
  // integer
  let int = 1;
  println!("{}", int);

  int += 10;
  println!("{}", int);

  int += 10;
  println!("{}", int);

  // floating
  let float = 1.0;
  println!("{}", float);

  float += 0.1;
  println!("{}", float);

  float += 0.1;
  println!("{}", float);
  
  return 0;
}
